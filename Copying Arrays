#include <iostream>
using namespace std;

int main() {
	float decimalNumbers[5] = { 1.2, 3.4, 5.0, 6.7, 8.9 };
	float decimalNumbers2[5];
	
	// Show Original Array
	cout << "Original Array" << endl;
	for (int i = 0; i < 5; i++) {
		cout << decimalNumbers[i] << " ";
	}
	cout << endl;

	// Copy
	for (int i = 0; i < 5; i++) {
		decimalNumbers2[i] = decimalNumbers[i];
	}

	// Second Array
	cout << "Second Array" << endl;
	for (int i = 0; i < 5; i++) {
		cout << decimalNumbers2[i] << " ";
	}
	cout << endl;
}
